{
  "name": "ui-models",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ui-models/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/ui-models",
        "main": "packages/ui-models/src/index.ts",
        "tsConfig": "packages/ui-models/tsconfig.lib.json",
        "assets": ["packages/ui-models/glb"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/ui-models/jest.config.ts"
      }
    }
  }
}
